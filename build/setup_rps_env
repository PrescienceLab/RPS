export RPSDIR=$RPS_DIR
export PPSDIR=$RPSDIR

export PATH=${RPSDIR}/Scripts:${RPSDIR}/bin/${RPS_ARCH}/${RPS_OS}:${RPSDIR}/ResearchScripts:$PATH

export LD_LIBRARY_PATH=${RPS_DIR}/lib/${RPS_ARCH}/${RPS_OS}:${LD_LIBRARY_PATH}

export PERLLIB=${RPS_DIR}/${PERLLIB}

export RPS_NEWSTYLEOUTPUT=yes

source $RPS_DIR/build/setup_default_ports


